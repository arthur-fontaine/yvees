{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "main": "./node_modules/.prebuild-rn/main.js",
  "scripts": {
    "dev:build-dry": "mkdir -p node_modules/.prebuild-rn",
    "dev:build-transform": "babel src --out-dir node_modules/.prebuild-rn --extensions .ts,.js,.tsx,.jsx,.cjs,.mjs --watch",
    "dev:expo": "expo start --tunnel --dev-client",
    "dev:agrume": "agrume --tunnel bore --watch",
    "dev": "run-p dev:*",
    "preprebuild": "pnpm -w run build && mkdir -p node_modules/.prebuild-rn",
    "prebuild": "babel src --out-dir node_modules/.prebuild-rn --extensions .ts,.js,.tsx,.jsx,.cjs,.mjs",
    "eas-build-post-install": "run-s preprebuild prebuild",
    "build-dev-clients": "eas build --profile development --platform all"
  },
  "dependencies": {
    "@badrap/valita": "^0.3.8",
    "@clerk/clerk-expo": "^1.1.8",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tamagui/card": "^1.110.1",
    "agrume": "^2.0.0",
    "db": "workspace:*",
    "diabolo": "npm:@jsr/arthur-fontaine__diabolo@^1.4.0",
    "expo": "^51.0.8",
    "expo-camera": "^15.0.10",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.19",
    "expo-font": "^12.0.7",
    "expo-linear-gradient": "^13.0.2",
    "expo-localization": "^15.0.3",
    "expo-secure-store": "^13.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "i18n-js": "^4.4.3",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.2.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@agrume/server": "^2.0.2",
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~18.2.45",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-preset-agrume": "^2.0.2",
    "dotenv": "^16.4.5",
    "eas-cli": "^10.1.0",
    "tsx": "^4.16.2"
  }
}
