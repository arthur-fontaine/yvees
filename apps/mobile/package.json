{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "main": "./node_modules/.prebuild-rn/main.js",
  "scripts": {
    "dev:build-dry": "mkdir -p node_modules/.prebuild-rn",
    "dev:build-transform": "babel src --out-dir node_modules/.prebuild-rn --extensions .ts,.js,.tsx,.jsx,.cjs,.mjs --watch",
    "dev:expo": "expo start --tunnel",
    "dev:agrume": "agrume --tunnel --watch",
    "dev": "run-p dev:*"
  },
  "dependencies": {
    "@agrume/server": "^2.0.0-beta.12",
    "agrume": "^2.0.0-beta.2",
    "expo": "^51.0.8",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~18.2.45",
    "babel-preset-agrume": "2.0.0-beta.3"
  }
}
